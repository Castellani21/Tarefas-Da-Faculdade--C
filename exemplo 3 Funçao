 #include <stdio.h>
 
 void VerificarParImpar(int num); // assinatura da funçao 
 
int main(){
        int numero;
        
        printf("digite um numero");
            scanf("%d", &numero);
            
            VerificarParImpar(numero); // chama a funçao 
            
    return 0;
}

void VerificarParImpar(int num){
    if(num % 2 == 0){
        printf("%d é par", num);
    }
        else{
            printf("%d é impar", num);
        }

    
  
}      